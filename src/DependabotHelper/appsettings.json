{
  "AllowedHosts": "*",
  "Dependabot": {
    "CacheLifetime": "00:10:00",
    "DisableCaching": false,
    "IncludeForks": false,
    "Labels": [ "dependencies" ],
    "MergeRetryWaits": [ "00:00:01", "00:00:02" ],
    "Users": [ "app/dependabot", "app/github-actions" ]
  },
  "GitHub": {
    "ClientId": "",
    "ClientSecret": "",
    "EnterpriseDomain": "",
    "Scopes": [ "repo", "user:email" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.DataProtection.KeyManagement.XmlKeyManager": "Error",
      "Microsoft.AspNetCore.DataProtection.Repositories.EphemeralXmlRepository": "Error",
      "Microsoft.AspNetCore.DataProtection.Repositories.FileSystemXmlRepository": "Error",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Warning"
    }
  },
  "Site": {
    "Description": "An application that helps manage Dependabot updates across multiple GitHub repositories.",
    "Domain": "",
    "Twitter": ""
  }
}
