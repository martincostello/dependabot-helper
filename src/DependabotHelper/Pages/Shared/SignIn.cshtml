@page "/sign-in"
@model SignInModel
@{
    ViewData["Title"] = "Sign In";
}
<p class="lead">
    Sign in with your GitHub account to manage your Dependabot updates.
</p>
<form action="~/sign-in" method="post">
    <button class="btn btn-lg btn-primary m-1" id="sign-in" type="submit">Sign in</button>
</form>
